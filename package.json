{
  "name": "voxel-walker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "coverage:open": "npx open-cli coverage/index.html",
    "test:lifecycle": "vitest run lifecycle",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "update:baselines": "node ./scripts/update-baselines.js",
    "profile": "node ./scripts/profile.js",
    "profile:baseline": "node --expose-gc --loader ts-node/esm dev/profiling/baseline-generator.ts"
  },
  "vitest": {
    "setupFiles": [
      "./tests/setupTests.ts"
    ]
  },
  "dependencies": {
    "@astrumforge/bvx-kit": "^1.41.0",
    "@react-three/drei": "10.7.7",
    "@react-three/fiber": "9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "open-simplex-noise": "^2.5.0",
    "postprocessing": "^6.38.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "three": "0.182.0",
    "zod": "^4.3.6",
    "zustand": "5.0.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.15.4",
    "eslint-plugin-unused-imports": "^4.4.1",
    "jsdom": "^28.0.0",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
